/* icon css  */

.searchbar-result{
    position:absolute;
    margin-top: 0;
    font-size: 14px;
    z-index: 99;
    width: 100%;

}
.searchbar-result .searchbar-result-shadow {
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: #000;
  opacity: 0.5;
  z-index: -1;
}
.searchbar-result:before{
    display: none;
}
.weui-cell{
    padding: 12px 15px 12px 35px;
}

.weui-cell__bd.active .confirm-icon{
  display: inline;
}

.confirm-icon{
  font-size: 0.7rem;
  color: green;
  display: none;
}
.searchbar-result-list{
  font-size: 0.8rem;
}
.searchbar-result-title{
  display: flex;
  justify-content: space-between
}
